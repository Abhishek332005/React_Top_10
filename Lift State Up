import React, { useState } from "react";

const Child = ({ count, setCount }) => (
  <button onClick={() => setCount(count + 1)}>Click Me</button>
);

function Parent() {
  const [count, setCount] = useState(0);
  return (
    <div>
      <h2>Count: {count}</h2>
      <Child count={count} setCount={setCount} />
    </div>
  );
}
export default Parent;
